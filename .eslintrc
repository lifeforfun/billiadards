{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
    }
  },
  "rules": {
    "semi": 0,
    "indent": ["error", 4, {"SwitchCase":1}],
    "no-console": "off",
    "no-debugger": "off"
  },
  "extends": "eslint:recommended",
  "globals":{
    // site_uri:true,
    // base_uri:true,
    "$":true,
    "jQuery":true,
    "QN":true,
    "workbench":true,
    "TOP":true,
    "createUrl":true,
    "SITE":true,
    "DEBUG":true,
    "PLATFORM":true,
    "layer":true
  }
}